var print = ""
var i = 0
var fizz = 3
var buzz = 5
var test1
var test2

function testFunc (x, y, z)
    output ("X: " + x)
    output ("Y: " + y)
    output ("Z: " + z)
end

loop i from 0 to 50
    i = i++
    print = ""

    if ( i % fizz == 0 ) then
        print = print + "Fizz"
    end

    if ( i % buzz == 0 ) then
        print = print + "Buzz"
    end

    if ( print == "") then
        print = i
    end

    output(print)
end

testFunc(5, 1, 2)


