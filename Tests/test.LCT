function FizzBuzz(startLoop, endLoop)
    var print = ""
    var i = 0
    var fizz = 3
    var buzz = 5

    loop i from startLoop to endLoop
        i = i++
        print = ""

        if ( i % fizz == 0 ) then
            print = print + "Fizz"
        end

        if ( i % buzz == 0 ) then
            print = print + "Buzz"
        end

        if ( print == "") then
            print = i
        end

        output(print)
    end
end

function main()
    output("------ Start! ------")
    FizzBuzz(0, 25)
    output("------ Stop! ------")
end

main()




